{
  "name": "youshark",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "analyze": "ANALYZE=true next build",
    "build:prod": "next build && next start",
    "build:cloudflare": "rm -rf .next out && NODE_ENV=production next build && cp _headers _redirects out/",
    "deploy:cloudflare": "npm run build:cloudflare && npx wrangler pages deploy out --project-name youshark-app",
    "dev:cloudflare": "npx wrangler dev",
    "preview:cloudflare": "npm run build:cloudflare && npm run dev:cloudflare"
  },
  "dependencies": {
    "@next/font": "14.0.4",
    "@paypal/paypal-js": "^8.3.0",
    "@paypal/paypal-server-sdk": "^1.1.0",
    "@woocommerce/woocommerce-rest-api": "^1.0.2",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "critters": "^0.0.23",
    "crypto-js": "^4.2.0",
    "framer-motion": "^10.16.16",
    "jsonwebtoken": "^9.0.2",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "styled-components": "^6.1.6"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.15",
    "@next/bundle-analyzer": "^15.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/styled-components": "^5.1.34",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "typescript": "5.3.3",
    "wrangler": "^4.33.2"
  },
  "description": "YouShark - YouTube growth services platform built with Next.js",
  "keywords": [
    "youtube",
    "social media",
    "marketing",
    "growth",
    "nextjs",
    "youshark"
  ],
  "author": "YouShark Team"
}
